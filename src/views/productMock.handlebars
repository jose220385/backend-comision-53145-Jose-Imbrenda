<main>
    <h1>Mock de preductos</h1>
     <div id="productsContainer">
                {{#each products}}
                <div class="productContainer" id={{this._id}}>
                    <h3>{{this.title}}</h3>
                    <p>{{this.code}}</p>
                    <p>{{this.description}}</p>
                    <p>{{this.price}}</p>
                    <div class="buttonContainer">
                    <a href="/products/{{this._id}}">Mas info</a>
                    </div>
                </div>
                {{/each}}
    </div>
</main>